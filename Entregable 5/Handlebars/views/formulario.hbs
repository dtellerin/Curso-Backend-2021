<h1 style="color:blue;">Ingrese Productos</h1>
<br>

<form action="/productos" method="post">

    <div class="form-group">
        <label for="title"><b>Nombre</b></label>
        <input id="title" class="form-control" type="text" name="title">
    </div>

    <div class="form-group">
        <label for="price"><b>Precio</b></label>
        <input id="price" class="form-control" type="number" name="price">
    </div>

    <div class="form-group">
        <label for="thumbail"><b>Foto URL</b></label>
        <input id="thumbail" class="form-control" type="text" name="thumbail">
    </div>

    <button class="btn btn-success mt-3 mb-5">Enviar</button>
    <a href="http://localhost:8080/productos" class="btn btn-info mt-3 mb-5" role="button">Ver Productos</a>
</form>